{
    "name": "proshop",
    "version": "1.0.0",
    "description": "MERN shopping cart app",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "update": "concurrently \"npm update\" \"npm update --prefix frontend\"",
        "start": "node backend/server",
        "server": "nodemon backend/server",
        "client": "npm start --prefix frontend",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "data:import": "node backend/seeder",
        "data:destroy": "node backend/seeder -d",
        "deploy": "gcloud app deploy",
        "build": "webpack --mode='production'",
        "pro-build": "ENVIRONMENT='production' gulp",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
        "pretty": "prettier --tab-width 4 --write \"**/*.{js,jsx,json,ts,html,css}\""
    },
    "author": "Brad Traversy",
    "license": "MIT",
    "dependencies": {
        "@tinymce/tinymce-react": "^4.3.0",
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "del": "^7.0.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-async-handler": "^1.2.0",
        "express-session": "^1.17.3",
        "fancy-log": "^2.0.0",
        "firebase": "^9.17.2",
        "fs": "^0.0.1-security",
        "fs-extra": "^11.1.0",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.3.0",
        "gulp-zip": "^5.1.0",
        "http-errors": "^2.0.0",
        "http-proxy-middleware": "^2.0.6",
        "jsonwebtoken": "^9.0.0",
        "mongodb": "^5.0.0",
        "mongoose": "^6.8.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "prettier": "^2.8.1",
        "tinymce": "^6.3.2",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-stream": "^7.0.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "concurrently": "^7.6.0",
        "nodemon": "^2.0.4"
    }
}
